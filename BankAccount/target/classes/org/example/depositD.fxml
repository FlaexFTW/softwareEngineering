<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.Transaction">

    <children>
        <Label layoutX="100.0" layoutY="75.0" text="Account Id:" />
        <TextField fx:id="accountIdText" layoutX="100.0" layoutY="99.0" />
        <Label layoutX="100.0" layoutY="143.0" text="Amount:" />
        <TextField fx:id="amountText" layoutX="101.0" layoutY="161.0" />
        <Button fx:id="depositBtn" layoutX="198.0" layoutY="211.0" mnemonicParsing="false" onAction="#deposit" text="Ok" />
        <Label fx:id="depositText" layoutX="300.0" layoutY="94.0" prefHeight="96.0" prefWidth="266.0" />
        <Button fx:id="menuBtn" layoutX="130.0" layoutY="211.0" mnemonicParsing="false" onAction="#menu" text="Menu" />
    </children>
</AnchorPane>

